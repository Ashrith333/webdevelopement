<%- include('partials/header') -%>

<p></p>
<div class= "container">
<div class = "jumbotron">
    <h1>Welcome to Campground</h1>
    <p>
        <a class ="btn btn-primary btn-large" href="/campgrounds/new"> Add image </a>
    </p>
</div>
 <div class="row">
    <% campground.forEach(function(campground){ %>
    <div div class="col-md-3 col-sm-6 ">
        <div class="thumbnail">
            <img src="<%= campground.image %>">
        </div>
        
        <h4> <%= campground.name %></h4>
    </div>
    <% }) %>
</div>
<%- include("partials/footer") -%>